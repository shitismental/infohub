{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/_next/static/$1"
    },
    {
      "src": "/(.*\\..+)$",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "http://152.53.18.199/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/media/(.*)",
      "dest": "http://152.53.18.199/media/$1"
    }
  ]
}